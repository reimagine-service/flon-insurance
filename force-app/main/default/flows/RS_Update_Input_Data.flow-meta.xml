<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>56.0</apiVersion>
    <environments>Default</environments>
    <formulas>
        <name>PersonDOB</name>
        <dataType>String</dataType>
        <expression>TEXT(MONTH({!$Record.PersonBirthdate}))+ &quot;/&quot; + TEXT(DAY({!$Record.PersonBirthdate}))+ &quot;/&quot; + TEXT(YEAR({!$Record.PersonBirthdate}))</expression>
    </formulas>
    <formulas>
        <name>PersonGender</name>
        <dataType>String</dataType>
        <expression>IF(TEXT({!$Record.Salutation}) = &quot;Mr.&quot;, &quot;M&quot;,(
 IF(TEXT({!$Record.Salutation}) = &quot;Mrs.&quot;, &quot;F&quot;,(
 IF(TEXT({!$Record.Salutation}) = &quot;Ms.&quot;, &quot;F&quot;,
		NULL)))))</expression>
    </formulas>
    <interviewLabel>RS_Update_Input_Data {!$Flow.CurrentDateTime}</interviewLabel>
    <label>RS_Update_Input_Data</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>OriginBuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processType>AutoLaunchedFlow</processType>
    <recordUpdates>
        <name>Update_Input_Data</name>
        <label>Update Input Data</label>
        <locationX>308</locationX>
        <locationY>288</locationY>
        <filterLogic>and</filterLogic>
        <filters>
            <field>AccountNumber__c</field>
            <operator>EqualTo</operator>
            <value>
                <elementReference>$Record.AccountNumber</elementReference>
            </value>
        </filters>
        <inputAssignments>
            <field>PatientDOB__c</field>
            <value>
                <elementReference>PersonDOB</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>PatientGender__c</field>
            <value>
                <elementReference>PersonGender</elementReference>
            </value>
        </inputAssignments>
        <inputAssignments>
            <field>PatientRegion__c</field>
            <value>
                <elementReference>$Record.ShippingCountry</elementReference>
            </value>
        </inputAssignments>
        <object>BTTS_InputData__x</object>
    </recordUpdates>
    <start>
        <locationX>50</locationX>
        <locationY>0</locationY>
        <filterLogic>1 AND (2 OR 3 OR 4)</filterLogic>
        <filters>
            <field>IsPersonAccount</field>
            <operator>EqualTo</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>PersonBirthdate</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>ShippingCountry</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <filters>
            <field>Salutation</field>
            <operator>IsChanged</operator>
            <value>
                <booleanValue>true</booleanValue>
            </value>
        </filters>
        <object>Account</object>
        <recordTriggerType>Update</recordTriggerType>
        <scheduledPaths>
            <connector>
                <targetReference>Update_Input_Data</targetReference>
            </connector>
            <pathType>AsyncAfterCommit</pathType>
        </scheduledPaths>
        <triggerType>RecordAfterSave</triggerType>
    </start>
    <status>Active</status>
</Flow>
